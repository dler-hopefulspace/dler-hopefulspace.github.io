"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n\n\n\n\n        <div class=\"row align-center justify-content-center\">\n\n            <div class=\"col-12 col-md-12\" mbr-if=\"showMainTitle\">\n\n                <h3 class=\"main-title align-center pb-3 mbr-semibold mbr-black mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".main-title\">關於我們</h3>\n\n            </div>\n\n\n            <div class=\"card card1 p-3 col-12 col-md-6 col-lg-3\">\n                <div class=\"card-wrapper\">\n\n                     <span mbr-icon class=\"mbr-iconfont mbrib-hot-cup\" mbr-if=\"showIcon\"></span>\n                   \n                    <div class=\"card-box\">\n                        <h5 class=\"card-title mbr-semibold pb-4 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">讀書會/自學課程</h5>\n                        <p class=\"card-text mbr-regular pt-1 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\"></p>\n                    </div>\n                </div>\n            </div>\n\n\n\n            <div class=\"card card2 p-3 col-12 col-md-6 col-lg-3\" mbr-if=\"cardsAmount > 1\">\n\n\n                <div class=\"card-wrapper\">\n\n                    <span mbr-icon class=\"mbr-iconfont mbrib-responsive\" mbr-if=\"showIcon\"></span>\n                    \n                    <div class=\"card-box\">\n                        <h5 class=\"card-title mbr-semibold pb-4 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">RWD形象網站設計製作<br></h5>\n                        <p class=\"card-text mbr-regular pt-1 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\"></p>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card card3 p-3 col-12 col-md-6 col-lg-3\" mbr-if=\"cardsAmount > 2\">\n                <div class=\"card-wrapper\">\n\n                    <span mbr-icon class=\"mbr-iconfont mbrib-video-play\" mbr-if=\"showIcon\"></span>\n                   \n                    <div class=\"card-box\">\n                        <h5 class=\"card-title mbr-semibold pb-4 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">影像剪輯</h5>\n                        <p class=\"card-text mbr-regular pt-1 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\"></p>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"card card4 p-3 col-12 col-md-6 col-lg-3\" mbr-if=\"cardsAmount > 3\">\n                <div class=\"card-wrapper\">\n\n                         <span mbr-icon class=\"mbr-iconfont mbrib-idea\" mbr-if=\"showIcon\"></span>\n                   \n                    <div class=\"card-box\">\n                        <h5 class=\"card-title mbr-semibold pb-4 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">logo/名片/banner設計製作</h5>\n                        <p class=\"card-text mbr-regular pt-1 mbr-black mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\"></p>\n                    </div>\n                </div>\n            </div>\n  \n\n        </div>\n    </div>\n</section>",
